{
  "name": "express-serve-writable",
  "version": "0.0.0",
  "description": "Express middleware for writing to the filesystem with PUT/DELETE requests",
  "main": "lib/serve_writable.js",
  "scripts": {
    "build": "npm run lint-js && npm test",
    "lint-js": "bash -c \"jshint lib/*.js\"",
    "test": "mocha lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ericlathrop/express-serve-writable.git"
  },
  "homepage": "https://github.com/ericlathrop/express-serve-writable",
  "bugs": {
    "url": "https://github.com/ericlathrop/express-serve-writable/issues"
  },
  "keywords": [
      "express",
      "middleware",
      "writable"
  ],
  "author": "Eric Lathrop <eric@ericlathrop.com> (http://ericlathrop.com/)",
  "license": "MIT",
  "dependencies": {
    "express": "^4.10.2",
    "serve-static": "^1.7.1"
  },
  "devDependencies": {
    "fs-mock": "^1.1.2",
    "jshint": "^2.5.10",
    "mocha": "^2.0.1",
    "rewire": "^2.1.3"
  },
  "jshintConfig": {
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "immed": true,
    "indent": 4,
    "latedef": "nofunc",
    "newcap": true,
    "noarg": true,
    "node": true,
    "noempty": true,
    "nonbsp": true,
    "nonew": true,
    "quotmark": "double",
    "unused": "strict",
    "trailing": true,
    "browser": true,
    "devel": true,
    "globalstrict": true,
    "mocha": true,
    "globals": {
    }
  }
}
